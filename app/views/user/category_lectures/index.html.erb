<div class="row">
  <% @lectures.each do |lecture| %>
    <% 
      assoziation = lecture.assoziations.first
   
      sections = lecture.materials
     
    %>
    <div class="col-xs-8 col-xs-offset-2">
      <div class="well well-lg">
        <div class="user-show-title">
         <%= lecture.name %> -
         <%= assoziation.code %> -
         <%= assoziation.instructor %> 
        </div>
        <div class="user-show-body">
          <%= lecture.description %>
         
        </div>
        <div class="user-show-actions">
          <% sections.each do |section| %>
            <span class="badge">
              <%= link_to section.name, user_section_materials_path(lecture, section) %> &nbsp;
            </span>
          
          <% end %>
          <%#= link_to 'Show Show all Sections', 
              user_faculty_courseofstudy_path(params[:faculty_id], courseofstudy),
             class: "btn btn-primary" %>
        </div>
      </div>
    </div> 
  <% end %>
